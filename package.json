{
  "name": "vue3-template",
  "version": "0.1.0",
  "description": "Template",
  "private": true,
  "engines": {
    "node" : "10.15.3",
    "npm"  : "6.11.3"
  },
  "scripts": {
    "serve": "vue-cli-service serve --host=0.0.0.0",
    "build": "vue-cli-service build",
    "docker:build": "docker build -t fid-access-replacement .",
    "docker:run": "docker run -it -p 5000:5000 --rm --name fid-access-replacement-1 fid-access-replacement",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "ssh": "docker-machine ssh fid-access-replacement"
  },
  "dependencies": {
    "firebase": "^5.10.1",
    "http-server": "^0.11.1",
    "proj4": "^2.5.0",
    "query-string": "^6.5.0",
    "register-service-worker": "^1.5.2",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@arcgis/webpack-plugin": "^4.10.5",
    "@jetstech/eslint-config-jetstech": "0.0.5",
    "@vue/cli-plugin-e2e-nightwatch": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-pwa": "^3.4.0",
    "@vue/cli-plugin-unit-mocha": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.8.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "faker": "^4.1.0",
    "husky": "^1.3.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-pug": "^1.0.7",
    "vue-template-compiler": "^2.5.21"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
